{
  "name": "api-ts-node",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/server.ts",
    "test": "mocha -r ts-node/register './src/tests/**/*.{test,spec}.ts' -t 10000 --exit",
    "test:coverage": "nyc npm run test",
    "db:reset": "tsc && npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "db:init": "tsc && npx sequelize db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all ",
    "dev": "nodemon -r dotenv/config src/server.ts"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.4",
    "@types/chai-http": "^4.2.0",
    "@types/cheerio": "^0.22.31",
    "@types/express": "^4.17.15",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/sequelize": "^4.28.14",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "cheerio": "^1.0.0-rc.12",
    "eslint": "^8.30.0",
    "mocha": "^10.2.0",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.5.2",
    "sinon": "^15.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.3.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "sequelize": "^6.28.0",
    "swagger-ui-express": "^4.6.1"
  }
}
